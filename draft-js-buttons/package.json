{
  "name": "@lagrowthmachine/draft-js-buttons",
  "version": "2.0.3",
  "description": "Buttons for DraftJS",
  "author": {
    "name": "Nik Graf",
    "email": "nik@nikgraf.com"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LaGrowthMachine/draft-js-plugins.git",
    "directory": "draft-js-buttons"
  },
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "editor",
    "wysiwyg",
    "draft",
    "react",
    "ux",
    "components",
    "widget",
    "react-component"
  ],
  "scripts": {
    "clean": "../node_modules/.bin/rimraf lib",
    "build": "yarn clean && yarn build:js && yarn build:ts",
    "build:js": "../node_modules/.bin/rollup --config ../rollup.config.js",
    "build:ts": "../node_modules/.bin/cpx src/*.d.ts lib/",
    "prepublish": "yarn version --patch && yarn build"
  },
  "license": "MIT",
  "dependencies": {
    "clsx": "^1.0.4"
  },
  "peerDependencies": {
    "draft-js": "^0.10.1 || ^0.11.0",
    "react": "^15.5.0 || ^16.0.0-rc",
    "react-dom": "^15.5.0 || ^16.0.0-rc"
  }
}
