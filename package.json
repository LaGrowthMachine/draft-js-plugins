    "publish-draft-js-emoji-plugin": "cd ./draft-js-emoji-plugin && npm publish && cd ../",
    "publish-draft-js-focus-plugin": "cd ./draft-js-focus-plugin && npm publish && cd ../",
    "publish-draft-js-hashtag-plugin": "cd ./draft-js-hashtag-plugin && npm publish && cd ../",
    "publish-draft-js-image-plugin": "cd ./draft-js-image-plugin && npm publish && cd ../",
    "publish-draft-js-inline-toolbar-plugin": "cd ./draft-js-inline-toolbar-plugin && npm publish && cd ../",
    "publish-draft-js-linkify-plugin": "cd ./draft-js-linkify-plugin && npm publish && cd ../",
    "publish-draft-js-mention-plugin": "cd ./draft-js-mention-plugin && npm publish && cd ../",
    "publish-draft-js-plugins-editor": "cd ./draft-js-plugins-editor && npm publish && cd ../",
    "publish-draft-js-plugins-utils": "cd ./draft-js-plugins-utils && npm publish && cd ../",
    "publish-draft-js-side-toolbar-plugin": "cd ./draft-js-side-toolbar-plugin && npm publish && cd ../",
    "publish-draft-js-static-toolbar-plugin": "cd ./draft-js-static-toolbar-plugin && npm publish && cd ../",
    "publish-draft-js-sticker-plugin": "cd ./draft-js-sticker-plugin && npm publish && cd ../",
    "publish-draft-js-undo-plugin": "cd ./draft-js-undo-plugin && npm publish && cd ../",
    "publish-draft-js-video-plugin": "cd ./draft-js-video-plugin && npm publish && cd ../",
    "publish-all": "yarn publish-draft-js-alignment && yarn publish-draft-js-anchor-plugin && yarn publish-draft-js-buttons && yarn publish-draft-js-counter-plugin && yarn publish-draft-js-divider-plugin && yarn publish-draft-js-drag-n-drop-plugin && yarn publish-draft-js-drag-n-drop-upload-plugin && yarn publish-draft-js-emoji-plugin && yarn publish-draft-js-focus-plugin && yarn publish-draft-js-hashtag-plugin && yarn publish-draft-js-image-plugin && yarn publish-draft-js-inline-toolbar-plugin && yarn publish-draft-js-linkify-plugin && yarn publish-draft-js-mention-plugin && yarn publish-draft-js-plugins-editor && yarn publish-draft-js-plugins-utils && yarn publish-draft-js-side-toolbar-plugin && yarn publish-draft-js-static-toolbar-plugin && yarn publish-draft-js-sticker-plugin && yarn publish-draft-js-undo-plugin && yarn publish-draft-js-video-plugin"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add",
      "eslint"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
