    "lint-staged": "^9.2.3",
    "matched": "^4.0.0",
    "mocha": "^3.0.2",
    "multer": "^1.3.0",
    "postcss-loader": "^1.3.3",
    "prettier": "^1.18.2",
    "prismjs": "^1.5.1",
